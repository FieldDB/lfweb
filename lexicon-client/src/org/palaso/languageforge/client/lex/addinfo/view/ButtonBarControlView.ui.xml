<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<ui:with type='org.palaso.languageforge.client.lex.common.I18nInterface' field='string' />
	<g:HTMLPanel>		
		<div class="lex-dc-row" >
			<div id="firefox-bug-fix">
				<div class="lex-dc-column cpadding5" id="c1">
					<b:Button ui:field='saveButton'  type="PRIMARY" icon="PENCIL"  text="{string.ButtonBarControlView_Save}" />
				</div>
				<div class="lex-dc-column cpadding5" id="c2">
				</div>
				<div class="lex-dc-column cpadding5" id="c3">
					<b:Button ui:field='prevButton' text="" title="{string.ButtonBarControlView_Previous}" width="20px"  icon="ARROW_LEFT"/>
					<b:Button ui:field='nextButton' text="" title="{string.ButtonBarControlView_Next}" width="20px" icon="ARROW_RIGHT" iconPosition="RIGHT" />
				</div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>
